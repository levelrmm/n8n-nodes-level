{
  "name": "@levelrmm/n8n-nodes-level",
  "version": "0.2.1",
  "description": "Official Level RMM integration for n8n to automate Level operations.",
  "license": "MIT",
  "homepage": "https://level.io",
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "automation",
    "level",
    "rmm",
    "level.io"
  ],
  "author": {
    "name": "Level",
    "email": "support@level.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Level-io/n8n-nodes-level.git"
  },
  "bugs": {
    "url": "https://github.com/Level-io/n8n-nodes-level/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "release": "n8n-node release",
    "prepublishOnly": "echo 'Skipping prerelease for CI'"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "strict": true,
    "credentials": [
      "dist/credentials/LevelApi.credentials.js",
      "dist/credentials/LevelWebhookSecret.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Level/Level.node.js",
      "dist/nodes/Level/LevelTrigger.node.js"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "devDependencies": {
    "@n8n/node-cli": "*",
    "@types/node": "22.10.1",
    "eslint": "9.32.0",
    "prettier": "3.6.2",
    "release-it": "^19.0.4",
    "typescript": "5.9.2"
  }
}
